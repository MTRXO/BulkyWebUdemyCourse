@model List<Category>

<h1>Category list </h1>

<table class="table table-bordered tabletable-striped" >
    <tr>

        <th>
            Category Name
        </th>
        <th>
            Display Order
        </th>
        <thead>
            <tbody>
                @foreach (var obj in Model.OrderBy(u => u.DisplayOrder))
                {
                    <tr>

                        <td>
                            @obj.Name
                        </td>
                        <td>@obj.DisplayOrder</td>

                    </tr>
                }

            </tbody>

        </thead>



    </tr>

</table>